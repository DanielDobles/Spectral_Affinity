                "import os, warnings, json, torch, torchaudio, librosa, glob, shutil, re, gc\n",
                "import numpy as np\n",
                "from tqdm.auto import tqdm\n",
                "from concurrent.futures import ThreadPoolExecutor\n",
                "from IPython.display import HTML, display\n",
                "from ultimate_pipeline import UltimateSunoMaster, MasteringEngine\n",
                "from sklearn.preprocessing import normalize\n",
                "from sklearn.cluster import KMeans as skKMeans\n",
                "\n",
                "warnings.filterwarnings('ignore')\n",
                "device = \"cuda\" if torch.cuda.is_available() else \"cpu\"\n",
                "\n",
                "# â”€â”€ Core Engines â”€â”€\n",
                "try:\n",
                "    from nnAudio.Spectrogram import CQT1992v2\n",
                "except:\n",
                "    !pip install -q nnAudio transformers deepfilternet matchering\n",
                "    from nnAudio.Spectrogram import CQT1992v2\n",
                "\n",
                "from transformers import Wav2Vec2FeatureExtractor, AutoModel\n",
                "try:\n",
                "    from cuml.cluster import KMeans as cuKMeans\n",
                "    HAS_CUML = True\n",
                "except:\n",
                "    HAS_CUML = False\n",
                "\n",
                "class SpectralMasterEngine:\n",
                "    def __init__(self, device='cuda', sr=24000):\n",
                "        self.device, self.sr = device, sr\n",
                "        self.cache = {}\n",
                "        self.cqt = CQT1992v2(sr=sr, n_bins=84).to(device)\n",
                "        self.processor = Wav2Vec2FeatureExtractor.from_pretrained(\"m-a-p/MERT-v1-95M\")\n",
                "        self.mert = AutoModel.from_pretrained(\"m-a-p/MERT-v1-95M\", trust_remote_code=True).to(device).eval()\n",
                "        \n",
                "    def process_batch(self, batch_data):\n",
                "        results = []\n",
                "        for p, y, d in batch_data:\n",
                "            with torch.no_grad():\n",
                "                inp = self.processor(y[:self.sr*15], sampling_rate=self.sr, return_tensors=\"pt\").input_values.to(self.device)\n",
                "                emb = self.mert(inp).last_hidden_state.mean(dim=1).squeeze().cpu().numpy()\n",
                "                # Simplified key/bpm for this summary block\n",
                "                results.append({'path': p, 'embedding': emb.tolist(), 'duration': d, 'bpm': 120.0, 'camelot': '08B', 'energy': 0.5})\n",
                "        return results\n",
                "\n",
                "def clean_name(n): return re.sub(r\"[\\-\\_\\.]+?\", \" \", re.sub(r\"^[\\w\\-]+?-\", \"\", os.path.basename(n).rsplit('.', 1)[0])).strip()\n",
                "\n",
                "def sequence_chromatic_set(tracks, target_duration):\n",
                "    if not tracks: return []\n",
                "    pool, current = list(tracks), tracks[0]\n",
                "    ordered, current_dur = [current], current['duration']\n",
                "    while pool and current_dur < target_duration:\n",
                "        pool.pop(0) # Logic placeholder\n",
                "        if not pool: break\n",
                "        next_t = pool[0]; ordered.append(next_t); current_dur += next_t['duration']; current = next_t\n",
                "    return ordered\n",
                "\n",
                "# â•”â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•—\n",
                "# â•‘  CONFIG - SLAVIC SONGS MASTER v6.2                           â•‘\n",
                "# â•šâ•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\n",
                "INPUT_DIR       = \"/kaggle/input/datasets/danieldobles/slavic-songs\"\n",
                "REFERENCE_TRACK = \"/kaggle/input/datasets/danieldobles/slavic-songs/REF.flac\"\n",
                "OUTPUT_DIR      = \"/kaggle/working/master_organized\"\n",
                "TEMP_DIR        = \"/kaggle/working/_temp_restore\"\n",
                "SET_DUR         = 75 * 60 \n",
                "\n",
                "STAGES = {\n",
                "    'neural_clean':    True,\n",
