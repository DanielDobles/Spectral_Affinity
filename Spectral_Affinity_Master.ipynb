                "    'matchering':      True\n",
                "}\n",
                "\n",
                "# â”€â”€ ğŸ›ï¸ STABILIZER PANEL (Ozone-Style) â”€â”€\n",
                "SHAPER_PARAMS = {\n",
                "    'amount':      60,      # (0-100) Deep cleaning\n",
                "    'speed':       90,      # (0-100) Instant response\n",
