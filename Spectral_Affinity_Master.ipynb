                "    'focus_low':   200,\n",
                "    'focus_high':  16000\n",
                "}\n",
                "\n",
                "print('\\nğŸ”§ Init Engines...')\n",
                "analyzer = SpectralMasterEngine(device=device)\n",
                "restorer = UltimateSunoMaster(device=device, stages=STAGES)\n",
                "mastering = MasteringEngine(reference_path=REFERENCE_TRACK)\n",
                "\n",
                "paths = []\n",
                "for ext in ['*.mp3', '*.wav', '*.flac', '*.m4a']:\n",
